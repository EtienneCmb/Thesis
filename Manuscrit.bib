  author = {Bastin, Julien and Deman, Pierre and David, Olivier and Gueguen, Ma{\"e}lle and Benis, Damien and Minotti, Lorella and Hoffman, Dominique and Combrisson, Etienne and Kujala, Jan and Perrone-Bertolotti, Marcela and Kahane, Philippe and Lachaux, Jean-Philippe and Jerbi, Karim},
  month = jan,
