  month = jan,
  year = {2008},
  pages = {i86--i95},
  file = {Liu et al_2008_Classification and feature selection algorithms for multi-class CGH data.pdf:/home/etienne/.zotero/zotero/zxayp3tr.default/zotero/storage/KJQA2XUK/Liu et al_2008_Classification and feature selection algorithms for multi-class CGH data.pdf:application/pdf},
